<h1>會員登入</h1>

<%= form_with(model: @user, url: sign_in_users_path, method: 'post', local: true) do |form| %>
    
    <%= form.label :email,'Email'%>
    <%= form.text_field :email %>

    <%= form.label :password, '密碼' %>
    <%= form.password_field :password %>
    <%= form.submit '登入' %>
<% end %>